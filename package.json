{
  "name": "surveyyay",
  "version": "1.0.0",
  "description": "An app that helps user create, send email campaign and receive feedback",
  "main": "index.js",
  "engines": {
    "node": "9.3.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js --ignore client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phuongha2012/SurveyYay.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phuongha2012/SurveyYay/issues"
  },
  "homepage": "https://github.com/phuongha2012/SurveyYay#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "mongoose": "^5.2.2",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "sendgrid": "^5.2.3",
    "stripe": "^6.3.0"
  }
}
